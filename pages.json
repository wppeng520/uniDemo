{
	"easycom": {
		"autoscan": true,
		"custom": {
			"m-(.*)": "@/components/my/m-$1/index.vue" ,// 匹配components目录内的vue文件
			"u-(.*)": "@/components/plug/uview/u-$1/index.vue" 
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-demo"
			}
		},
		{
			"path": "pages/tree/index",
			"style": {
				"navigationBarTitleText": "树"
			}
		},
		{
			"path": "pages/list/index",
			"style": {
				"navigationBarTitleText": "列表"
			}
		},
		{
			"path": "pages/loading/index",
			"style": {
				"navigationBarTitleText": "加载中"
			}
		},
		{
			"path": "pages/empty/index",
			"style": {
				"navigationBarTitleText": "空数据"
			}
		},
		{
			"path": "pages/file/index",
			"style": {
				"navigationBarTitleText": "附件上传"
			}
		},
		{
			"path": "pages/form/index",
			"style": {
				"navigationBarTitleText": "表单"
			}
		},
		{
			"path": "pages/publicSelect/userListMulti",
			"style": {
				"navigationBarTitleText": "人员选择"
			}
		},
		{
			"path": "pages/publicSelect/userListRadio",
			"style": {
				"navigationBarTitleText": "人员选择"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
